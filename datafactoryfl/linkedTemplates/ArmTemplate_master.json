{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactoryfl"
		},
		"CloudSalesSQLSource_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "ConnectionStringCloudSales"
		},
		"CloudStreamingSQLSource_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "ConnectionStringCloudStreaming"
		},
		"DataLakeSink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalake@ohmdwflwagner.dfs.core.windows.net/"
		},
		"MyAzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://dfkvfl.vault.azure.net/"
		},
		"MyCosmosDB_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "CosmosDB"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DataLakeSink_properties_typeProperties_url": {
						"value": "[parameters('DataLakeSink_properties_typeProperties_url')]"
					},
					"MyAzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('MyAzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"CloudSalesSQLSource_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('CloudSalesSQLSource_properties_typeProperties_connectionString_secretName')]"
					},
					"CloudStreamingSQLSource_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('CloudStreamingSQLSource_properties_typeProperties_connectionString_secretName')]"
					},
					"MyCosmosDB_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('MyCosmosDB_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}