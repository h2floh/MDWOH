{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactoryfl"
		},
		"CloudSalesSQLSource_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "ConnectionStringCloudSales"
		},
		"CloudStreamingSQLSource_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "ConnectionStringCloudStreaming"
		},
		"DataLakeSink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalake@ohmdwflwagner.dfs.core.windows.net/"
		},
		"FourthCoffeeServer_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Rentals"
		},
		"FourthCoffeeServer_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "flwagner"
		},
		"MyAzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://dfkvfl.vault.azure.net/"
		},
		"MyCosmosDB_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "CosmosDB"
		},
		"VanArsdelSQLServer_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "VanArsdelSQLServer"
		},
		"FourthCoffee_properties_typeProperties_linkedInfo_resourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/3b559740-5a58-4416-9d55-89cef0e9b9d1/resourcegroups/cloud-shell-storage-northeurope/providers/Microsoft.DataFactory/factories/StouthFactories/integrationruntimes/integrationRuntime4coffee"
		},
		"VanArsdel_properties_typeProperties_linkedInfo_resourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/3b559740-5a58-4416-9d55-89cef0e9b9d1/resourcegroups/cloud-shell-storage-northeurope/providers/Microsoft.DataFactory/factories/StouthFactories/integrationruntimes/integrationRuntime2"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DataLakeSink_properties_typeProperties_url": {
						"value": "[parameters('DataLakeSink_properties_typeProperties_url')]"
					},
					"MyAzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('MyAzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"FourthCoffee_properties_typeProperties_linkedInfo_resourceId": {
						"value": "[parameters('FourthCoffee_properties_typeProperties_linkedInfo_resourceId')]"
					},
					"VanArsdel_properties_typeProperties_linkedInfo_resourceId": {
						"value": "[parameters('VanArsdel_properties_typeProperties_linkedInfo_resourceId')]"
					},
					"CloudSalesSQLSource_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('CloudSalesSQLSource_properties_typeProperties_connectionString_secretName')]"
					},
					"CloudStreamingSQLSource_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('CloudStreamingSQLSource_properties_typeProperties_connectionString_secretName')]"
					},
					"FourthCoffeeServer_properties_typeProperties_host": {
						"value": "[parameters('FourthCoffeeServer_properties_typeProperties_host')]"
					},
					"FourthCoffeeServer_properties_typeProperties_userId": {
						"value": "[parameters('FourthCoffeeServer_properties_typeProperties_userId')]"
					},
					"MyCosmosDB_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('MyCosmosDB_properties_typeProperties_connectionString_secretName')]"
					},
					"VanArsdelSQLServer_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('VanArsdelSQLServer_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}